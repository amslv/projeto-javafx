<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="450.0" prefWidth="700.0"
	stylesheets="@modena_dark.css" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLCadastroProdutoController">
	<children>
		<GridPane fx:id="gridPane" layoutX="15.0" layoutY="29.0"
			prefHeight="300.0" prefWidth="670.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="329.0"
					minWidth="10.0" prefWidth="193.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0"
					minWidth="10.0" prefWidth="477.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label fx:id="lblNomeProduto" text="Nome do Produto:" />
				<TextField fx:id="tfNomeProduto" GridPane.columnIndex="1" />
				<Label fx:id="lblDescricaoProduto" text="Descrição do Produto:"
					GridPane.rowIndex="1" />
				<TextArea fx:id="taDescricaoProduto" prefHeight="200.0"
					prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<Label fx:id="lblQuantidadeItens" text="Quantidade de Itens:"
					GridPane.rowIndex="2" />
				<TextField fx:id="tfQuantidadeItens" GridPane.columnIndex="1"
					GridPane.rowIndex="2" />
				<Label fx:id="lblValorCompra" text="Valor de Compra (R$):"
					GridPane.rowIndex="3" />
				<TextField fx:id="tfValorCompra" GridPane.columnIndex="1"
					GridPane.rowIndex="3" />
				<Label fx:id="lblValorVenda" text="Valor de Venda (R$):"
					GridPane.rowIndex="4" />
				<TextField fx:id="tfValorVenda" GridPane.columnIndex="1"
					GridPane.rowIndex="4" />
			</children>
		</GridPane>
		<Button fx:id="btnSalvarProduto" layoutX="90.0" layoutY="356.0"
			mnemonicParsing="false" onAction="#handleSalvarProduto" prefHeight="40.0"
			prefWidth="190.0" text="Salvar Produto" />
		<Button fx:id="btnCancelar" layoutX="408.0" layoutY="356.0"
			mnemonicParsing="false" prefHeight="40.0" prefWidth="190.0" text="Cancelar" />
	</children>
</AnchorPane>
